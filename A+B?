#include<iostream>
#include<string>
using namespace std;
int main(){
    int t;
    cin>>t;
    for(int i=0;i<t;i++){
        string a;
        cin>>a;
        char arr[3];
        arr[0]=a[0];
        arr[1]=a[1];
        arr[2]=a[2];
        int a=arr[0]-'0';
        int b=arr[2]-'0';
        cout<<a+b<<endl;
    }
}
