#include<bits/stdc++.h>
using namespace std;
int main(){
    int year;
    cin>>year;
    while(true){
        year+=1;
        string syear=to_string(year);
        set<char>set_year(begin(syear),end(syear));
        
        if(set_year.size() == 4){
            cout<<year<<endl;
            break;
        }
    }
}
