#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--) { 
        int n;
        cin>>n;
        int mihai=0,bianca=0;
        for(int i=0;i<n;i++){  
            int candy;
            cin>>candy;
            if(candy%2==0) {
                mihai+= candy;  
            } else{
                bianca+=candy;  
            }
        }
        if (mihai>bianca) {
            cout<<"YES"<< endl;
        } else {
            cout<<"NO"<< endl;
        }
    }

   
}
