#include<iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int r;
    bool isHard=false;
    for(int i=0;i<n;i++){
        cin>>r;
        if(r==1){
            isHard=true;
            break;
        }
    }
    if(isHard){
        cout<<"HARD"<<endl;
    }
    else{
        cout<<"EASY"<<endl;
    }
}
